{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "cypress",
      "node"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "."
  },
  "include": [
    "cypress",
    "cypress.config.ts"
  ]
}